{
    "name": "norman-huth/lura",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NormanHuth\\Lura\\": "src/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "support": {
        "issues": "https://github.com/Muetze42/lura/issues",
        "docs": "https://github.com/Muetze42/lura",
        "source": "https://github.com/Muetze42/lura"
    },
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "illuminate/cache": "^9.43|^10.0",
        "illuminate/config": "^9.43|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/events": "^9.0|^10.0",
        "illuminate/filesystem": "^9.43|^10.0",
        "league/flysystem": "^3.11",
        "norman-huth/helpers-collection-php": "^1.3"
    },
    "bin": [
        "bin/lura"
    ],
    "config": {
        "sort-packages": true
